  <style>
    #output {
      font-family: Arial, sans-serif;
      font-size: 40px;
      white-space: pre-wrap;
      text-shadow: 1px 2px 1px black;
      color: white;
      font-weight: bolder;
    }

    .fade-in {
      display: inline-block;
      opacity: 0;
      animation: fadeIn 0.5s forwards;
    }

    @keyframes fadeIn {
      from {
        opacity: 0;
        transform: translateY(-10px);
      }

      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    #animate {
      animation-name: displayAnimate;
      animation-duration: 1s;
      animation-iteration-count: infinite;
    }

    @keyframes displayAnimate {
      from {
        color: red;
      }

      to {
        color: whitesmoke;
      }
    }

    #animateBtn {
      animation-name: displayAnimateBtn;
      animation-duration: 1s;
      animation-iteration-count: infinite;
    }

    @keyframes displayAnimateBtn {
      from {
        background-color: crimson;
      }

      to {
        background-color: #7971ea;
      }
    }

    /* canvas logic starts */
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    /* body {
      background-color: #000;
      height: 100vh;
      overflow: hidden;
      cursor: none;
    } */

    /* canvas {
      display: block;
    } */
    /*  */
    /* Allow body to scroll */
    body {
      /* background-color: #000; */
      height: auto;
      overflow: visible;
      /* cursor: none; */
    }

    /* Keep canvas fixed without blocking scroll */
    canvas {
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      z-index: -1;
      width: 100vw;
      height: 100vh;
      cursor: none;
    }

    /* canvas logic ends */

              /* glimpse image height starts */
              .gallery-image {
            width: 100%; /* Full width */
            aspect-ratio: 16/9; /* Maintain aspect ratio */
            object-fit: cover; /* Ensure the image covers its container */
          }
        
          @media (max-width: 768px) {
            .gallery-image {
              aspect-ratio: 4/3;
            }
          }
        
          @media (max-width: 576px) {
            .gallery-image {
              aspect-ratio: 1/1;
            }
          }
        /* glimpse image height ends */

  </style>

