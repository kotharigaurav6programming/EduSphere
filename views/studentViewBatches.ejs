<!DOCTYPE html>
<html lang="en">
  <head>
    <base href="/">
    <title>EduSphere</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    
    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="fonts/icomoon/style.css">

    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/jquery-ui.css">
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/owl.theme.default.min.css">
    <link rel="stylesheet" href="css/owl.theme.default.min.css">

    <link rel="stylesheet" href="css/jquery.fancybox.min.css">

    <link rel="stylesheet" href="css/bootstrap-datepicker.css">

    <link rel="stylesheet" href="fonts/flaticon/font/flaticon.css">

    <link rel="stylesheet" href="css/aos.css">

    <link rel="stylesheet" href="css/style.css">
    
  </head>
  <body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">
  
  <div class="site-wrap">

    <div class="site-mobile-menu site-navbar-target">
      <div class="site-mobile-menu-header">
        <div class="site-mobile-menu-close mt-3">
          <span class="icon-close2 js-menu-toggle"></span>
        </div>
      </div>
      <div class="site-mobile-menu-body"></div>
    </div>
   


    <header class="site-navbar py-4 js-sticky-header site-navbar-target" role="banner">
      
      <div class="container-fluid">
        <div class="d-flex align-items-center">
          <div class="site-logo mr-auto"><a href="index.html">EduSphere</a></div>
          <div>
            <nav class="site-navigation position-relative text-right" role="navigation">
                <%-include('./studentHomeHeader.js')%>
            </nav>
           <a href="#" class="d-inline-block d-lg-none site-menu-toggle js-menu-toggle text-black float-right">
            <span class="icon-menu h3"></span></a>

          </div>
        </div>
      </div>
      
    </header>

   
    <div class="site-section overlay" style=" margin-top: -1px;">
     
      <div class="container" style="padding-top: 70px;">
        <div class="row mb-5 justify-content-center">
          <div class="col-lg-7 text-center" data-aos="fade-up" data-aos-delay="">
            <h4 class="text-black mb-4">My Batches</h4>
            <span><%=message%></span>
          </div>
        </div>

        <style>
          #intQ:hover{
            transition: 1s;
            transform: scale(1);
          }
        </style>
        
        <div class="row" style="margin-top: -50px;" data-aos="fade-up" data-aos-delay="">
          <%if(allocatedBatchData.length==0){%>
            No Record Found
          <%}else{%>
            <%allocatedBatchData.forEach((obj)=>{%> 
            <div id="intQ" class="col-md-5 col-lg-5" style="margin:4%;border-radius:10px;box-shadow: 1px 1px 10px 1px; padding: 10px;">
              
                <span class="text-black">
                  <strong class="text-primary">Batch Id : </strong><%=obj.batchId%><br>
                  <strong class="text-primary">Course Name : </strong><%=obj.courseName%><br>
                  <strong class="text-primary">Trainer Name : </strong><%=obj.trainerName%><br>
                  <strong class="text-primary">Enroll Id : </strong><%=obj.enrollId%><br>
                  <strong class="text-primary">Name : </strong><%=obj.name%><br>
                  <strong class="text-primary">Email : </strong><%=obj.email%><br>
                  <strong class="text-primary">Start Date : </strong><%=new Date(obj.startDate).toISOString().split("T")[0]%><br>
                  <strong class="text-primary">Start Time : </strong><%=obj.startTime%><br>
                  <strong class="text-primary">End Time : </strong><%=obj.endTime%><br>
                  <strong class="text-primary">Batch Duration : </strong><%=obj.batchDuration%><br>
                </span>
                <form action="/student/studentViewAssignments" method="post">
                  <input type="hidden" name="batchId" id="batchId" value="<%=obj.batchId%>">
                  <button class="btn btn-primary btn-block">View Assignments</button>
                </form>
              </div>
            <%})%>
          <%}%>
        </div>
      </div>
    </div>
    <%- include('./footerSection.ejs')%>
  </div> <!-- .site-wrap -->

  <script src="js/jquery-3.3.1.min.js"></script>
  <script src="js/jquery-migrate-3.0.1.min.js"></script>
  <script src="js/jquery-ui.js"></script>
  <script src="js/popper.min.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <script src="js/owl.carousel.min.js"></script>
  <script src="js/jquery.stellar.min.js"></script>
  <script src="js/jquery.countdown.min.js"></script>
  <script src="js/bootstrap-datepicker.min.js"></script>
  <script src="js/jquery.easing.1.3.js"></script>
  <script src="js/aos.js"></script>
  <script src="js/jquery.fancybox.min.js"></script>
  <script src="js/jquery.sticky.js"></script>

  
  <script src="js/main.js"></script>
    
  </body>
</html>